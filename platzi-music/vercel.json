{
  "version": 2,
  "routes": [
    { "src": "(.*).js", "dest": "/$1.js" },
    { "src": "(.*).json", "dest": "/$1.json" },
    { "src": "(.*).png", "dest": "/$1.png" },
    { "src": "(.*).css", "dest": "/$1.css" },
    { "src": "(.*).ico", "dest": "/$1.ico" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
